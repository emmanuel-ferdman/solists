{
  "compilerOptions": {
    "lib": ["es6"],
    "module": "commonjs",
    "target": "es6",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "outDir": "dist"
  },
  "include": [
    "src/"
  ]
}
